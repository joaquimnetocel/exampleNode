{
	"imports": {
		"#pathJavascriptExamples/*": "./folderSource/folderExamples/*",
		"#pathTypescriptExamples/*": "./folderDist/folderExamples/*",
		"#pathModules/*": "./folderDist/folderModules/*"
	},
	"type": "module",
	"name": "examplenode",
	"version": "1.0.0",
	"description": "EXAMPLE OF NODE PROJECT",
	"main": "./folderDist/index.js",
	"scripts": {
		"hello": "node ./folderSource/folderExamples/exampleHelloWorld.js",
		"tsc-nodemon": "nodemon --exec \"tsc --skipLibCheck && node %npm_config_jsfile%\"",
		"esbuild-nodemon": "nodemon --exec \"etsc --config=esbuild.development.config.cjs && node %npm_config_jsfile%\"",
		"database": "prisma db push --schema=./prisma/schema.prisma && prisma generate --schema=./prisma/schema.prisma",
		"studio": "prisma studio --schema=./prisma/schema.prisma",
		"tsc-dev": "cross-env dotenvDevelopmentMode=true npm run tsc-nodemon --jsfile=./folderDist/index.js\"",
		"esbuild-dev": "cross-env dotenvDevelopmentMode=true npm run esbuild-nodemon --jsfile=./folderDist/index.js\"",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"esbuild": "^0.14.51",
		"esbuild-node-tsc": "^1.8.6",
		"eslint": "^8.20.0",
		"nodemon": "^2.0.19",
		"prisma": "^4.1.1",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"@prisma/client": "^4.1.1",
		"altair-express-middleware": "^4.5.3",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.1",
		"ejs": "^3.1.8",
		"express": "^4.18.1",
		"express-graphql": "^0.12.0",
		"graphql-modules": "^2.1.0",
		"graphql-playground-html": "^1.6.30",
		"graphql-subscriptions": "^2.0.0",
		"graphql-ws": "^5.9.1",
		"ws": "^8.8.1"
	}
}
